// Resolution Graph
digraph {
	"~A(Lion)" [color=blue shape=doublecircle]
	"B(Ada)"
	"~A(Lion)" -> "B(Ada)"
	"A(x)|B(Ada)" -> "B(Ada)"
	"A(Umbrella)"
	"B(Ada)" -> "A(Umbrella)"
	"A(Umbrella)|~B(Ada)" -> "A(Umbrella)"
	"A(Umbrella)|A(x)"
	"A(x)|B(Ada)" -> "A(Umbrella)|A(x)"
	"A(Umbrella)|~B(Ada)" -> "A(Umbrella)|A(x)"
}
